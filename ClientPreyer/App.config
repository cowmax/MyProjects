      </setting>
      <setting name="userCfgUrl" serializeAs="String">
        <value>http://web.jingoal.com/mgt/workbench/v1/get_userconfig?code={0}&amp;ouri={1}</value>
      </setting>
    </ClientPreyer.Properties.Settings>
  </userSettings>
</configuration>